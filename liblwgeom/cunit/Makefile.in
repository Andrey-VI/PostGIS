
LWGEOM=..
CUNIT_LIB=@CUNIT_PATH@/lib/libcunit.a
CUNIT_HEADER=@CUNIT_PATH@/include

LIBLWGEOM=$(LWGEOM)/liblwgeom.a

OBJS =	cu_tester.o \
	cu_algorithm.o

#---------------------------------------------------------------

INCLUDES = -I$(CUNIT_HEADER) -I$(LWGEOM)
CFLAGS = -g $(MSDEFINES) $(INCLUDES)
LIBS = $(LIBLWGEOM) $(CUNIT_LIB)
LDFLAGS = 

cu_tester: $(OBJS) $(LIBS)

test: cu_tester
	./cu_tester

check: test

clean:
	@rm $(OBJS)
	@rm cu_tester
